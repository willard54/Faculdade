//Fornecidos três valores, a, b e c, escreva um programa que retorne quantos dos três são iguais. A resposta pode ser 3 (todos iguais), 2 (apenas um diferente) ou 0 (todos diferentes).

const qual = (a) => (b) => (c) => {
  if (a === b && b === c)
    return 3
  if (a !== b && b !== c && c !== a)
    return 0
  else return 2
}

console.log(qual(7)(2)(7))
